extend ../../layouts/layout.jade

block title
	| Dashboard - Sell

block setting
	.hide-breadcrumb

block content
	.dashboard-page
		.dashboard-page__header.fluid-header
			.fluid-header__content
				.fluid-header__tabs
					.fluid-header__tab: a(href = "../dashboard-buy") quản lý mua
					.fluid-header__tab.selected: a(href = "../dashboard-sell") quản lý bán
					.fluid-header__tab: a(href = "../dashboard-inbox") tin nhắn
					.fluid-header__tab: a(href = "../dashboard-customize") tuỳ chọn
		.dashboard-page__content
			.dashboard-page__filter
				ul.db-filter.cols.selector
					each title in ["tổng quan", "đang bán", "đã bán", "chưa bán", "bản thảo", "đã huỷ"]
						li.cols__item.db-filter__item(class = (title === "tổng quan" ? "selected" : ""))
							a.db-filter__link(href = "#", class = selectedClass).button.button--line.button--small= title
			//- SECTION DIVIDER ----------
			section.db-section-wrapper: .db-section
				header.db-section__header
					h1.db-section__title Đang bán
				form.form.sell-filter-form.cols
					.cols__item.sell-filter-form__search-box: .search-box
						.search-box__icon.icon
							i.icon__icon.icon__icon--img
								img(src = "../../assets/icons/ic_search_black_24dp.png", width = "20", height = "20")
						input(type = "text").search-box__text-box
						button.search-box__button tìm
					.cols__item.sell-filter-form__select: select.select.select--full-width
						option tất cả
					.cols__item.sell-filter-form__select: select.select.select--full-width
						option tất cả
				table.table.sell-table
					colgroup
						col(style = "width: 200px")
						col
						col(style = "width: 100px")
						col(style = "width: 100px")
						col(style = "width: 40px")
						col(style = "width: 100px")
						col(style = "width: 100px")
						col(style = "width: 40px")
						col(style = "width: 40px")
						col(style = "width: 40px")
						col(style = "width: 40px")
						col(style = "width: 100px")
						col(style = "width: 100px")
					thead
						tr
							th hình ảnh
							th tên sản phẩm
							th mã phiên
							th kiểu đăng
							th SL
							th giá niêm yết
							th giá hiện tại
							th: img(src = "../../assets/icons/ic_assignment_return_black_24dp.png", width = "15", height = "15")
